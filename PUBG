def login():
	os.system('clear')
	try:
		toket = open('login.txt','r')
		menu() 
	except (KeyError,IOError):
		os.system('clear')
		print logo
		jalan(' \033[1;92mâš  Warning âš  \033[1;96mDo Not Use Your Personal Account' )
